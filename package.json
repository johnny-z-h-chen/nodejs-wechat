{
  "name": "visiongo-backend-wechat",
  "version": "1.0.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "eslint --ext .js ./ && nodemon app.js --exec babel-node --presets es2015,stage-2",
    "debug": "babel-node --presets es2015,stage-2 --inspect app.js",
    "lint": "eslint --ext .js ./",
    "lint:fix": "eslint --ext .js ./ --fix"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "@log4js-node/slack": "^1.0.0",
    "axios": "^0.21.1",
    "body-parser": "^1.13.3",
    "btoa-atob": "^0.1.2",
    "compression": "^1.5.2",
    "content-type": "^1.0.4",
    "cors": "^2.7.1",
    "crypto": "^1.0.1",
    "ejs": "^3.0.1",
    "express": "^4.13.3",
    "jsonschema": "^1.2.4",
    "log4js": "^6.4.0",
    "memory-cache": "^0.2.0",
    "qs": "^6.9.1",
    "raw-body": "^2.4.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "require-all": "^3.0.0",
    "resource-router-middleware": "^0.6.0",
    "sha1": "^1.1.1",
    "swagger-ui-express": "^4.1.2",
    "uuid": "^3.3.3",
    "validator": "^12.0.0",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jsdoc": "^3.6.3",
    "nodemon": "^1.9.2",
    "prettier": "^1.19.1"
  }
}
